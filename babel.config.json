{
  "presets": [
    ["@babel/preset-env"],
    ["@babel/preset-typescript", { "isTSX": true, "allExtensions": true }],
    ["@babel/preset-react", { "runtime": "automatic" }],
    ["@wyw-in-js"]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@app": "./src/app",
          "@pages": "./src/pages",
          "@components": "./src/components",
          "@features": "./src/features",
          "@shared": "./src/shared"
        }
      }
    ]
  ]
}
