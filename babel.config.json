{
  "presets": [
    ["@babel/preset-typescript", { "isTSX": true, "allExtensions": true }],
    ["@babel/preset-react", { "runtime": "automatic" }],
    ["@babel/preset-env"]
  ],
  "plugins": [
    [
      "module-resolver",
      {
        "root": ["./src"],
        "alias": {
          "@app": "./src/app",
          "@pages": "./src/pages",
          "@components": "./src/components"
        }
      }
    ]
  ]
}
